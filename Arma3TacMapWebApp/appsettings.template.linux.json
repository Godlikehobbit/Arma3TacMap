{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Arma3TacMapContext": "Data Source=/var/sqlite/Arma3TacMap.db" // Path to the database file (sqlite, file will be created if missing)
  },
  "Steam": { "Key": "xxxxxxxxxxxxxx" }, // Your steam api key (get it from https://steamcommunity.com/dev/apikey)
  "Admins": [
    "https://steamcommunity.com/openid/id/xxxxxxxxxxxxxxxxxxx" // Your steam id
  ],
  "Chrome": "/usr/bin/chromium-browser", // Path to chromium-browser (used to generate previews of maps)
  "UnixKeysDirectory": "/var/aspnet-keys", // Path to a secure folder to store cookie protection keys
  "CorsOrigins": []
}
